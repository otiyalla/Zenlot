{
  "cli": {
    "version": ">= 16.7.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "environment": "development",
      "env": {
        "EXPO_PUBLIC_AUTH_TOKEN": "zenlot-auth-token",
        "EXPO_PUBLIC_AUTH_REFRESH_TOKEN": "zenlot-auth-token-refresh",
        "EXPO_PUBLIC_USER_ID": "zenlot-user-id"
      }
    },
    "ios-simulator": {
      "ios": {
        "simulator": true
      },
      "extends": "development"
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "ENVIRONMENT": "preview"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "ENVIRONMENT": "production"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
